  "license": "MIT",
  "bugs": {
    "url": "https://github.com/allangrds/laxus-nextjs-blog/issues"
  },
  "homepage": "https://github.com/allangrds/laxus-nextjs-blog#readme",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf node_modules",
    "commit": "cz",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "typecheck": "tsc --noEmit --incremental false",
    "eslint": "eslint './pages/' --ext .json,.js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "next": "12.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.6.4",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.2.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "prettier": "^2.7.1",
    "typescript": "4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
